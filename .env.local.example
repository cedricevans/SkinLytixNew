# Local environment example for SkinLytix
# Copy this to .env.local and fill values before running locally

# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-public-anon-key    # used for dev proxy fetch headers
VITE_SUPABASE_PUBLISHABLE_KEY=your-public-publishable-key  # fallback if ANON key not set

# Dev-only: when true, Vite will proxy /functions to Supabase Edge Functions
VITE_USE_FUNCTIONS_PROXY=true

# Feature flags
VITE_ENABLE_EXPLAIN_INGREDIENTS=false
